    "ports": {
      "input": {
        "domain": {
          "type": "string"
        },
        "rrtype": {
          "enum": [
            "A",
            "AAAA",
            "MX",
            "TXT",
            "SRV",
            "PTR",
            "NS",
            "CNAME"
          ]
        }
      },
      "output": {
        "results": {
          "type": "object"
        }
      }
    },
    "dependencies": {
      "dns": "latest"
