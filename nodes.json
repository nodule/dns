{
  "dns.lookup": {
    "fn": "output = [dns, 'lookup', input.domain, input.family]\n",
    "ports": {
      "input": {
        "domain": {
          "type": "string"
        },
        "family": {
          "type": "integer"
        }
      },
      "output": {
        "results": {
          "type": "object"
        }
      }
    },
    "dependencies": {
      "dns": "latest"
    },
    "description": "dns lookup",
    "name": "dns.lookup"
  },
  "dns.resolve": {
