{
  "name": "dns",
  "description": "DNS components for ChiÏ‡",
  "version": "0.0.3",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-dns.git"
  },
  "nodes": [
    {
      "ports": {
        "input": {
          "domain": {
            "type": "string"
          },
          "family": {
            "type": "integer"
          }
        },
        "output": {
          "results": {
            "type": "object"
          }
        }
      },
      "dependencies": {
        "dns": "latest"
      },
      "description": "dns lookup",
      "name": "lookup",
      "ns": "dns",
      "fn": "output = [dns, 'lookup', input.domain, input.family]\n"
    },
    {
      "ports": {
        "input": {
          "domain": {
            "type": "string"
          },
          "rrtype": {
            "enum": [
              "A",
              "AAAA",
              "MX",
              "TXT",
              "SRV",
              "PTR",
              "NS",
              "CNAME"
            ]
          }
        },
        "output": {
          "results": {
            "type": "object"
          }
        }
      },
      "dependencies": {
        "dns": "latest"
      },
      "description": "dns resolve",
      "name": "resolve",
      "ns": "dns",
      "fn": "output = [dns, 'resolve', input.domain, input.rrtype]\n"
    },
