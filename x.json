{
  "name": "dns",
  "description": "DNS components for ChiÏ‡",
  "version": "0.0.1",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-dns.git"
  },
  "nodes": {
    "lookup": {
      "fn": "output = [dns, 'lookup', input.domain, input.family]\n",
      "ns": "dns",
      "ports": {
        "input": {
          "domain": {
            "type": "string"
          },
          "family": {
            "type": "integer"
          }
        },
        "output": {
          "results": {
            "type": "object"
          }
        }
      },
      "dependencies": {
        "dns": "latest"
      },
      "description": "dns lookup",
      "name": "lookup"
    },
    "resolve": {
      "fn": "output = [dns, 'resolve', input.domain, input.rrtype]\n",
      "ns": "dns",
      "ports": {
        "input": {
          "domain": {
            "type": "string"
          },
          "rrtype": {
            "enum": [
              "A",
              "AAAA",
              "MX",
              "TXT",
              "SRV",
              "PTR",
              "NS",
              "CNAME"
            ]
          }
        },
        "output": {
          "results": {
            "type": "object"
          }
        }
      },
      "dependencies": {
        "dns": "latest"
      },
      "description": "dns resolve",
      "name": "resolve"
    },
